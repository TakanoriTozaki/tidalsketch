sound "tozabc*2" # n "<2 4>" # pan "<0.0 0.25 0.5 0.75 1.0>" # shape "<0.0 0.25 0.5>" # legato "<0.25>" # lpf "1000 3000" # resonance "<0.25 0.75>",
